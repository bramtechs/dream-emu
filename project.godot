; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Spatial",
"class": "PillarTool",
"language": "GDScript",
"path": "res://addons/component_view/tools/PillarTool.gd"
}, {
"base": "Spatial",
"class": "StairTool",
"language": "GDScript",
"path": "res://addons/component_view/tools/StairTool.gd"
} ]
_global_script_class_icons={
"PillarTool": "res://addons/component_view/assets/pillar.png",
